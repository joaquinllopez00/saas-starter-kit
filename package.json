{
  "name": "base-kit",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "build": "NODE_ENV=production remix vite:build",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "init": "npx tsx scripts/setup",
    "test": "vitest",
    "setup:permissions": "npx tsx scripts/add-roles-permissions.ts",
    "email:dev": "email dev -d ./app/emails -p 3001",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:seed": "npx tsx app/drizzle/scripts/seed.ts",
    "db:browser": "npx drizzle-kit studio --verbose",
    "stripe:setup": "npx tsx scripts/setup-stripe.ts",
    "stripe:setup-webhook": "npx tsx scripts/setup-stripe.ts --webhook",
    "stripe:listen": "stripe listen --events checkout.session.completed,customer.subscription.updated,customer.subscription.deleted --forward-to localhost:3000/stripe/webhook",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ./app",
    "lint": "eslint --fix --max-warnings 0 ./app",
    "ci": "npm run typecheck && npm run lint && npm run format"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-email/components": "0.0.17",
    "@remix-run/express": "^2.12.2",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@sentry/profiling-node": "^8.18.0",
    "@sentry/remix": "^8.18.0",
    "@sentry/vite-plugin": "^2.21.1",
    "@tanstack/react-table": "^8.10.7",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "gray-matter": "^4.0.3",
    "ioredis": "^5.4.1",
    "isbot": "^3.6.8",
    "jotai": "^2.7.1",
    "lucide-react": "^0.292.0",
    "nodemailer": "6.9.9",
    "nodemailer-brevo-transport": "^2.1.0",
    "otpauth": "^9.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
