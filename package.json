    "@remix-run/eslint-config": "^2.2.0",
    "@remix-run/testing": "2.12.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.44.7",
